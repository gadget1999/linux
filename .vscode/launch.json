  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Stock",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "query", "MSFT"
      ],
      "env": {
        "IR2MQTT_MAPPING": "${relativeFileDirname}/config-samples/ir2mqtt.json",
        "MQTT_SERVER": "test.mosquitto.org",
        "MQTT_PORT": "1883",
        "MQTT_USER": "",
        "MQTT_PASSWORD": ""
      },
      "type": "python"
    },
    {
      "name": "Python: IR2MQTTBridge",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "0:0"
      ],
      "env": {
        "IR2MQTT_MAPPING": "${relativeFileDirname}/config-samples/ir2mqtt.json",
        "MQTT_SERVER": "test.mosquitto.org",
        "MQTT_PORT": "1883",
        "MQTT_USER": "",
        "MQTT_PASSWORD": ""
      },
      "type": "python"
    },
    {
      "name": "Python: MQTT2InfluxDBBridge",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "${relativeFileDirname}/config-samples/mqtt2influxdb.cfg"
      ],
      "env": {
      },
      "debugOptions": [
      ],
      "type": "python"
    },
    {
      "name": "Python: MP3",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "list",
        "~"
      ],
      "type": "python"
    },
    {
      "name": "Python: m3u8-downloader",
      "request": "launch",
      "program": "${file}",
      "args": [
        "https://test.com/playlist_audio.m3u8",
        "https://test.com/playlist_1080.m3u8",
        "Test Video"
      ],
      "env": {
        "M3U8_WORK_DIR": "/tmp",
        "FFMPEG": "ffmpeg"
      },
      "console": "integratedTerminal",
      "type": "python"
    },
    {
      "name": "Python: WebMonitor",
      "request": "launch",
      "program": "${file}",
      "args": [
        "${relativeFileDirname}/web-monitor/web-monitor.cfg"
      ],
      "env": {
        "SENDGRID_API_KEY": "INVALID_KEY_PLACE_HOLDER"
      },
      "console": "integratedTerminal",
      "type": "python"
    }
  ]
}